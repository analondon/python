#conditionals
user = 'admin'
logged_in = True
if user == 'admin' and logged_in :
    print('admin page')
else :
    print('Bad creds')
admin page

condition = False
if condition : #so weird???
    print('eval to True')
else :
    print('eval to false')
eval to false
condition = None #(or 0, or an empty sequence, or an empty mapping)
if condition :
    print('eval to True')
else :
    print('eval to false')
eval to false

